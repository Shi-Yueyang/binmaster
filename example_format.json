{
  "endianness": "little",
  "description": "Example binary format with various data types",
  "fields": [
    {
      "name": "header",
      "type": "struct",
      "fields": [
        {
          "name": "magic",
          "type": "uint32"
        },
        {
          "name": "version",
          "type": "uint16"
        },
        {
          "name": "flags",
          "type": "uint16"
        }
      ]
    },
    {
      "name": "name",
      "type": "string",
      "size": 32,
      "encoding": "utf-8"
    },
    {
      "name": "data_count",
      "type": "uint32"
    },
    {
      "name": "data_array",
      "type": "array",
      "length_field": "data_count",
      "element_type": "float32"
    },
    {
      "name": "nested_data",
      "type": "array",
      "size": 2,
      "element_type": "struct",
      "element_fields": [
        {
          "name": "id",
          "type": "uint32"
        },
        {
          "name": "value",
          "type": "float64"
        }
      ]
    }
  ]
}